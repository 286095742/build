+		opp-microvolt = <730000>;
+	};
+
+	opp-500000000 {
+		opp-hz = <0x00 500000000>;
+		opp-microvolt = <730000>;
+	};
+
+	opp-667000000 {
+		opp-hz = <0x00 667000000>;
+		opp-microvolt = <730000>;
+	};
+	
+	opp-2016000000 {
+		status = "disabled";
+	};
+	
+	opp-2100000000 {
+		status = "disabled";
+	};
+};
